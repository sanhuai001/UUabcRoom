<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.40" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.60" />

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.005" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.42" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.28" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.72" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.09" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.91" />

    <ImageView
        app:layout_constraintTop_toTopOf="@id/line1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/ic_room_sdk_bg_room_in_tip"
        android:layout_width="0dp"
        android:layout_height="0dp" />

    <ImageView
        android:id="@+id/iv_bell"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/ic_room_sdk_room_bell"
        app:layout_constraintDimensionRatio="W,97:105"
        app:layout_constraintLeft_toRightOf="@id/line2"
        app:layout_constraintRight_toLeftOf="@id/line3" />

    <ImageView
        android:layout_width="33dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/line10"
        android:background="@drawable/ic_room_sdk_start1"
        app:layout_constraintDimensionRatio="H,27:27" />

    <ImageView
        android:layout_width="16dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/line1"
        app:layout_constraintRight_toLeftOf="@id/line2"
        android:background="@drawable/ic_room_sdk_start2"
        app:layout_constraintDimensionRatio="H,16:17" />

    <ImageView
        android:id="@+id/iv_star3"
        android:layout_width="16dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/line5"
        app:layout_constraintRight_toLeftOf="@id/line3"
        android:background="@drawable/ic_room_sdk_start3"
        app:layout_constraintDimensionRatio="H,16:17" />

    <ImageView
        android:layout_width="14dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/line5"
        app:layout_constraintRight_toLeftOf="@id/iv_star3"
        android:background="@drawable/ic_room_sdk_start4"
        app:layout_constraintDimensionRatio="H,16:17" />

    <TextView
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/iv_bell"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/line6"
        android:textSize="@dimen/common_text_size_xx_large"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/activity_rank_over_class_str2" />


    <TextView
        android:id="@+id/tv_msg"
        app:layout_constraintTop_toBottomOf="@id/line6"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/line13"
        android:textSize="@dimen/common_text_size_xx_large"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_good_tips_str" />

    <com.uuabc.classroomlib.widget.GradeView
        android:id="@+id/gv_diamond"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:starCount="5"
        android:gravity="center"
        app:clickable="false"
        app:starEmpty="@drawable/ic_room_sdk_diamond_white"
        app:starFill="@drawable/ic_room_sdk_diamond_blue"
        app:starImageHeight="30dp"
        app:starImageWidth="35dp"
        app:starPadding="10dp"
        app:starStep="0"
        app:layout_constraintDimensionRatio="W,265:38"
        app:layout_constraintLeft_toLeftOf="@id/line15"
        app:layout_constraintRight_toRightOf="@id/line16"
        app:layout_constraintTop_toBottomOf="@id/line13"
        app:layout_constraintBottom_toTopOf="@id/line14">

    </com.uuabc.classroomlib.widget.GradeView>

    <ImageButton
        android:id="@+id/btn_click"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/room_sdk_selector_over_dialog_button"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/line11"
        app:layout_constraintTop_toBottomOf="@id/line12"
        app:layout_constraintDimensionRatio="W,300:88" />


</androidx.constraintlayout.widget.ConstraintLayout>